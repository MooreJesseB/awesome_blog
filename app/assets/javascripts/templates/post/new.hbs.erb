{{#each result}}
<div class="two column row">
  <div class="twelve wide column">
    <div class="ui blue segment stacked fade-first">
      <h4 class="ui header">
        <a href="/users/{{user_id}}/posts/{{id}}">
        {{title}}
      </a>
      <p>{{created_at}}</p>
      <p>{{content}}</p>
    </h4>
    </div>
  </div>
  <div class="twelve wide column"></div>
</div>
{{/each}}
